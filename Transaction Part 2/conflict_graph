digraph ConflictGraph {
	graph [rankdir=LR ranksep=2]
	node [color=lightblue style=filled]
	T1
	T2
	T1
	T3
	T1
	T3
	T2
	T3
	T2
	T3
	T2
	T3
	T2
	T3
	T1 -> T2 [label="A-(#2,#3)"]
	T1 -> T3 [label="A-(#1,#8)"]
	T1 -> T3 [label="A-(#2,#8)"]
	T2 -> T3 [label="A-(#3,#8)"]
	T2 -> T3 [label="B-(#4,#7)"]
	T2 -> T3 [label="B-(#5,#6)"]
	T2 -> T3 [label="B-(#5,#7)"]
}
